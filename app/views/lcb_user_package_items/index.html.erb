<p id="notice"><%= notice %></p>

<h1>Lcb User Package Items</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Content</th>
      <th>User</th>
      <th>Summary</th>
      <th>Begin time</th>
      <th>End time</th>
      <th>Tips</th>
      <th>Used</th>
      <th>Used time</th>
      <th>Id</th>
      <th>Owner</th>
      <th>Create uid</th>
      <th>Write uid</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lcb_user_package_items.each do |lcb_user_package_item| %>
      <tr>
        <td><%= lcb_user_package_item.name %></td>
        <td><%= lcb_user_package_item.type %></td>
        <td><%= lcb_user_package_item.content %></td>
        <td><%= lcb_user_package_item.user_id %></td>
        <td><%= lcb_user_package_item.summary %></td>
        <td><%= lcb_user_package_item.begin_time %></td>
        <td><%= lcb_user_package_item.end_time %></td>
        <td><%= lcb_user_package_item.tips %></td>
        <td><%= lcb_user_package_item.used %></td>
        <td><%= lcb_user_package_item.used_time %></td>
        <td><%= lcb_user_package_item.id %></td>
        <td><%= lcb_user_package_item.owner_id %></td>
        <td><%= lcb_user_package_item.create_uid %></td>
        <td><%= lcb_user_package_item.write_uid %></td>
        <td><%= lcb_user_package_item.amount %></td>
        <td><%= link_to 'Show', lcb_user_package_item %></td>
        <td><%= link_to 'Edit', edit_lcb_user_package_item_path(lcb_user_package_item) %></td>
        <td><%= link_to 'Destroy', lcb_user_package_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

