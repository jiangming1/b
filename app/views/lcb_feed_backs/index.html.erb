<p id="notice"><%= notice %></p>

<h1>Lcb Feed Backs</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>User</th>
      <th>Content</th>
      <th>Owner</th>
      <th>Create uid</th>
      <th>Write uid</th>
      <th>Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lcb_feed_backs.each do |lcb_feed_back| %>
      <tr>
        <td><%= lcb_feed_back.status %></td>
        <td><%= lcb_feed_back.user_id %></td>
        <td><%= lcb_feed_back.content %></td>
        <td><%= lcb_feed_back.owner_id %></td>
        <td><%= lcb_feed_back.create_uid %></td>
        <td><%= lcb_feed_back.write_uid %></td>
        <td><%= lcb_feed_back.id %></td>
        <td><%= link_to 'Show', lcb_feed_back %></td>
        <td><%= link_to 'Edit', edit_lcb_feed_back_path(lcb_feed_back) %></td>
        <td><%= link_to 'Destroy', lcb_feed_back, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lcb Feed Back', new_lcb_feed_back_path %>
