<p id="notice"><%= notice %></p>

<h1>Lcb Financing Target Tags</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Financing target</th>
      <th>Owner</th>
      <th>Create uid</th>
      <th>Write uid</th>
      <th>Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lcb_financing_target_tags.each do |lcb_financing_target_tag| %>
      <tr>
        <td><%= lcb_financing_target_tag.tag %></td>
        <td><%= lcb_financing_target_tag.financing_target_id %></td>
        <td><%= lcb_financing_target_tag.owner_id %></td>
        <td><%= lcb_financing_target_tag.create_uid %></td>
        <td><%= lcb_financing_target_tag.write_uid %></td>
        <td><%= lcb_financing_target_tag.id %></td>
        <td><%= link_to 'Show', lcb_financing_target_tag %></td>
        <td><%= link_to 'Edit', edit_lcb_financing_target_tag_path(lcb_financing_target_tag) %></td>
        <td><%= link_to 'Destroy', lcb_financing_target_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

