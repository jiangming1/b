<p id="notice"><%= notice %></p>

<h1>Lcb Cash Coupons</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Amount</th>
      <th>Summary</th>
      <th>Begin time</th>
      <th>End time</th>
      <th>Conditions</th>
      <th>Used amount</th>
      <th>Financing target bid</th>
      <th>Def</th>
      <th>Id</th>
      <th>Owner</th>
      <th>Create uid</th>
      <th>Write uid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lcb_cash_coupons.each do |lcb_cash_coupon| %>
      <tr>
        <td><%= lcb_cash_coupon.name %></td>
        <td><%= lcb_cash_coupon.user_id %></td>
        <td><%= lcb_cash_coupon.amount %></td>
        <td><%= lcb_cash_coupon.summary %></td>
        <td><%= lcb_cash_coupon.begin_time %></td>
        <td><%= lcb_cash_coupon.end_time %></td>
        <td><%= lcb_cash_coupon.conditions %></td>
        <td><%= lcb_cash_coupon.used_amount %></td>
        <td><%= lcb_cash_coupon.financing_target_bid_id %></td>
        <td><%= lcb_cash_coupon.def_id %></td>
        <td><%= lcb_cash_coupon.id %></td>
        <td><%= lcb_cash_coupon.owner_id %></td>
        <td><%= lcb_cash_coupon.create_uid %></td>
        <td><%= lcb_cash_coupon.write_uid %></td>
        <td><%= link_to 'Show', lcb_cash_coupon %></td>
        <td><%= link_to 'Edit', edit_lcb_cash_coupon_path(lcb_cash_coupon) %></td>
        <td><%= link_to 'Destroy', lcb_cash_coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lcb Cash Coupon', new_lcb_cash_coupon_path %>
