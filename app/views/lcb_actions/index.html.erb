<p id="notice"><%= notice %></p>

<h1>Lcb Actions</h1>

<table>
  <thead>
    <tr>
      <th>Uri</th>
      <th>Action name</th>
      <th>Id</th>
      <th>Create uid</th>
      <th>Write uid</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lcb_actions.each do |lcb_action| %>
      <tr>
        <td><%= lcb_action.uri %></td>
        <td><%= lcb_action.action_name %></td>
        <td><%= lcb_action.id %></td>
        <td><%= lcb_action.create_uid %></td>
        <td><%= lcb_action.write_uid %></td>
        <td><%= lcb_action.owner_id %></td>
        <td><%= link_to 'Show', lcb_action %></td>
        <td><%= link_to 'Edit', edit_lcb_action_path(lcb_action) %></td>
        <td><%= link_to 'Destroy', lcb_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lcb Action', new_lcb_action_path %>
