<p id="notice"><%= notice %></p>

<h1>Lcb Templates</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Type</th>
      <th>Content</th>
      <th>Owner</th>
      <th>Create uid</th>
      <th>Write uid</th>
      <th>Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lcb_templates.each do |lcb_template| %>
      <tr>
        <td><%= lcb_template.code %></td>
        <td><%= lcb_template.name %></td>
        <td><%= lcb_template.type %></td>
        <td><%= lcb_template.content %></td>
        <td><%= lcb_template.owner_id %></td>
        <td><%= lcb_template.create_uid %></td>
        <td><%= lcb_template.write_uid %></td>
        <td><%= lcb_template.id %></td>
        <td><%= link_to 'Show', lcb_template %></td>
        <td><%= link_to 'Edit', edit_lcb_template_path(lcb_template) %></td>
        <td><%= link_to 'Destroy', lcb_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lcb Template', new_lcb_template_path %>
